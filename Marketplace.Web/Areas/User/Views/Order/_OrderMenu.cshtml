@model Marketplace.Web.Areas.User.Models.Order.OrderListViewModel
@using Marketplace.Web.HtmlHelpers

<ul class="account-inner-list" id="account-inner-list">

    <li class="account-inner-menu-item ">@Html.ActionLink("Заказы (покупка)", "BuyList", "Order", null, new { @class = "account-inner-menu-link " + Html.IsSelected(actions: "BuyList", controllers: "Order") })</li>
    <li class="account-inner-menu-item">@Html.ActionLink("Заказы (продажа)", "SellList", "Order", null, new { @class = "account-inner-menu-link " + Html.IsSelected(actions: "SellList", controllers: "Order") })</li>
</ul>
